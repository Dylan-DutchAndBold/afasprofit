{
    "name": "megachriz/afasprofit",
    "description": "An API to connect to Afas Profit written in PHP.",
    "keywords": ["afas", "profit"],
    "license": "GPL-3.0",
    "type": "library",
    "authors": [
        {
            "name": "Youri van Koppen",
            "email": "megachriz@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/dependency-injection": ">=2.7",
        "openlss/lib-array2xml": "~0.0",
        "drupal/core-plugin": "^8.4",
        "symfony/finder": "^3.4",
        "hanneskod/classtools": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Afas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afas\\Tests\\": "tests/"
        }
    }
}
